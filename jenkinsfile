@Library('mylibrary')_
pipeline
{
    agent any
    stages
    {
        stage('contdownload_master')
        {
            steps
            {
                script
                {
                    cicd.gitdownload("maven")
                }
            }
        }
        stage('contbuild_master')
        {
            steps
            {
                script
                {
                    cicd.build()
                }
            }
        }
        stage('contdeploy_master')
        {
            steps
            {
                script
                {
                    cicd.tomcatdeploy("http://172.31.36.3:8080","testapp")
                }
            }
        }
        stage('conttest_master')
        {
            steps
            {
                script
                {
                    cicd.gitdownload("functionaltesting")
                    cicd.selenium("Declarativepipelinesharedlibrary")
                }
            }
        }
        stage('contdelivary_master')
        {
            steps
            {
                script
                {
                    cicd.tomcatdeploy("http://172.31.39.255:8080","prodapp")
                }
            }
        }
    }
} 
